    "deepmerge": "^4.3.1",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.4",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^5.0.4",
    "uglify-es": "^3.3.9"
